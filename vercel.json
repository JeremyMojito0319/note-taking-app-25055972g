{
  "version": 2,
  "builds": [
    {
      "src": "src/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/main.py"
    },
    {
        "src": "/favicon.ico",
        "dest": "/src/static/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/src/static/index.html"
    }
  ]
}
